public class accenture {
    public static void main(String[] args) {
        int arr [] = {1, 2, 4, 5, 6};
        int n = arr.length + 1;
        int sum = 0;

        for(int i : arr) {
            sum += i;
        }
        int first = (n * (n+1))/2 ;
        int sec = sum;

        int res = first - sec;
        System.out.println(res);
    }
}
