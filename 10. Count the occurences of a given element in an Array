import java.util.Scanner;

public class accenture {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int arr [] = new int[n];
        for(int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
        }
        int element = sc.nextInt();

        int count = 0;

        for(int j = 0; j < arr.length; j++) {
            if(arr[j] == element) {
                count++;
            }
        }
        System.out.println(count);
    }
}
