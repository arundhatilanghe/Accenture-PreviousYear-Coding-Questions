public class accenture {
    public static void main(String[] args) {
        int input1 = 4;
        int input2 = 5;
        int arr [] = {1, 3, 5, 8};

        int count = 0;

        for(int i = 0; i < input1; i++) {
            for(int j = i; j < input1; j++) {
                int small = arr[i];
                int large = arr[j];
                
                if(small + large <= input2) {
                    count++;
                }
            }
        }
        System.out.println(count);
    }
}
