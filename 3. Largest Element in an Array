public class accenture {
    public static void main(String[] args) {
        
        int arr [] = {5, 7, 8, 3, 1};
        int max = arr[0];

        for(int i = 0; i < arr.length; i++) {
            if (arr[i] > max) {
                max = arr[i];
            }
        }
        System.out.println(max);
    }
}
